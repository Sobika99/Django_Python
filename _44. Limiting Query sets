>>> a = [10, 20,30]
>>> type(a)
<class 'list'>

>>> a [1:2]
[20]
>>> a[1:3]
[20, 30]
>>> a[0:3:2]
[10, 30]

>>> JobPost.objects.filter()[1:2]
<QuerySet [<JobPost: Second job post>]>

>>> JobPost.objects.filter()[1:3]
<QuerySet [<JobPost: Second job post>, <JobPost: third job post>]>

>>> all_job_post = JobPost.objects.filter()
>>> all_job_post [1:2]
<QuerySet [<JobPost: Second job post>]>

